{
    "routes": [
      {
        "route": "/signin",
        "serve": "/signin.html",
        "statusCode": 200
      },
      {
        "route": "/signup",
        "serve": "/signup.html",
        "statusCode": 200
      }
    ],
    "rewrites": [
      {
        "source": "/signin.html",
        "destination": "/signin.html"
      },
      {
        "source": "/signup.html",
        "destination": "/signup.html"
      }
    ]
  }